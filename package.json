{
  "name": "clean-tdd-finances",
  "version": "1.3.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=production && node dist/main/server.js",
    "build": "rimraf dist && npx tsc",
    "debug": "set NODE_ENV=dev && nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "set NODE_ENV=dev && npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "set NODE_ENV=dev && jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "set NODE_ENV=dev && jest --passWithNoTests --runInBand",
    "test:unit": "set NODE_ENV=dev && npm test -- --watch -c jest-unit-config.js",
    "test:integration": "set NODE_ENV=dev && npm test -- --watch -c jest-integration-config.js",
    "test:staged": "set NODE_ENV=dev && jest --passWithNoTests --onlyChanged --watch --detectOpenHandles",
    "test:ci": "set NODE_ENV=dev && npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.13.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^5.6.0",
    "nodemon": "^3.0.1",
    "validator": "^13.9.0"
  }
}
